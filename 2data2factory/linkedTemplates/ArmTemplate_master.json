{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "2data2factory"
		},
		"secondaryudemydatalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'secondaryudemydatalake'"
		},
		"Synapticsequel_shallowsqlpool_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Synapticsequel_shallowsqlpool'"
		},
		"secondaryudemydatalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://secondaryudemydatalake2.dfs.core.windows.net/"
		},
		"Synapticsequel_shallowsqlpool_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "synapticsequel.sql.azuresynapse.net"
		},
		"Synapticsequel_shallowsqlpool_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "shallowsqldb"
		},
		"Synapticsequel_shallowsqlpool_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladminuser"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "2data2factory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"secondaryudemydatalake_accountKey": {
						"value": "[parameters('secondaryudemydatalake_accountKey')]"
					},
					"Synapticsequel_shallowsqlpool_password": {
						"value": "[parameters('Synapticsequel_shallowsqlpool_password')]"
					},
					"secondaryudemydatalake_properties_typeProperties_url": {
						"value": "[parameters('secondaryudemydatalake_properties_typeProperties_url')]"
					},
					"Synapticsequel_shallowsqlpool_properties_typeProperties_server": {
						"value": "[parameters('Synapticsequel_shallowsqlpool_properties_typeProperties_server')]"
					},
					"Synapticsequel_shallowsqlpool_properties_typeProperties_database": {
						"value": "[parameters('Synapticsequel_shallowsqlpool_properties_typeProperties_database')]"
					},
					"Synapticsequel_shallowsqlpool_properties_typeProperties_userName": {
						"value": "[parameters('Synapticsequel_shallowsqlpool_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}